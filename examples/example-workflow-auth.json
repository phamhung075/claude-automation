    "test_files": [
      "tests/auth/test_jwt.py",
      "tests/auth/test_middleware.py",
      "tests/auth/test_password.py"
    ],
    "documentation": [
      "docs/api/authentication.md"
    ]
  },

  "success_criteria": {
